<div>
<% @users.each do |user| %>
    <div>
        <%= user.name %>
    </div>
    <div>
        <%= user.email %>
    </div>
    <% if user.id != @current_user.id %>
        <div>
            <%# <%= link_to("編集", "/posts/#{user.id}/edit") %>
            <%= link_to("削除", "/admin/#{user.id}/destroy", {method: "post"}) %>
        </div>
    <% end %>
<% end %>
</div>